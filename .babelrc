{
    "presets": [
        [
            "@babel/preset-env",
            {
                "loose": false,
                "modules": "commonjs",
                "spec": true,
                "targets": {
                    "browsers": [
                        "last 3 Chrome versions",
                        "last 3 Firefox versions",
                        "Safari >= 10",
                        "Explorer >= 11",
                        "Edge >= 12",
                        "iOS >= 10",
                        "Android >= 6"
                    ]
                },
                "useBuiltIns": "usage",
                "corejs": 3,
                "debug": false
            },
            "@vue/app"
        ]
    ],
    "env": {
        "test": {
            "plugins": [
                "istanbul"
            ]
        }
    },
    "plugins": [
        "@babel/plugin-transform-modules-umd"
    ],
    "comments": false
}
